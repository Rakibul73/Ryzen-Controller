{
  "name": "ryzencontroller",
  "productName": "RyzenController",
  "version": "1.5.0",
  "description": "A minimal Electron application to use ryzenAdj through a friendly interface.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds --ignore=\".*-builds\" --icon=\"assets/icon.ico\" --win32metadata.requested-execution-level=requireAdministrator",
    "build": "node installers/windows/createinstaller.js"
  },
  "repository": "https://gitlab.com/le.storm1er/ryzen-controller",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "le.storm1er",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.4",
    "electron-packager": "^13.1.0",
    "electron-winstaller": "2.5.2"
  },
  "dependencies": {
    "electron-settings": "^3.2.0",
    "uikit": "^3.0.3"
  }
}
